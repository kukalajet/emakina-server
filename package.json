{
  "name": "emakina-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts --verbose",
    "start:dev": "NODE_ENV=development nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:nodedebug": "nest start --debug --watch",
    "prestart:prod": "rimraf dist && npm run build --verbose",
    "start:prod": "NODE_ENV=production node dist/main.js --verbose",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "heroku-postbuild": "echo Skip builds on Heroku"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.18",
    "@nestjs/core": "^7.6.18",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^4.8.1",
    "@nestjs/typeorm": "^7.1.5",
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.18",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.2",
    "class-validator": "^0.9.1",
    "config": "^3.1.0",
    "google-auth-library": "^6.1.3",
    "into-stream": "^7.0.0",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.0",
    "passport-facebook-token": "^4.0.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.11.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^7.0.1",
    "typeorm": "^0.2.34",
    "typescript": "^3.2.4",


    
    "@nestjs/testing": "^6.0.0",
    "@types/jest": "^23.3.13",
    "@types/multer": "^1.4.6",
    "@types/passport-facebook-token": "^0.4.34",
    "@types/supertest": "^2.0.7",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "^23.10.5",
    "tsconfig-paths": "^3.7.0",
    "tslint": "5.12.1"
  },
  "devDependencies": {
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
